#include <iostream>

using namespace std;

int main()
{
    int arr2[5];

    for(int i=0;i<5;i++){
        cout << "Input Integer" << i+1 << ":";
        cin >> arr2[i];

    }
        cout << endl << "Odd Numbers!:" << endl;
    for(int i=0;i<5;i++){
            if (arr2[i]%2==1){
                cout << arr2[i] << " ";
            }

    }
cout << endl << "Even Numbers!:" << endl;
    for(int i=0;i<5;i++){
            if (arr2[i]%2==0){
                cout << arr2[i] << " ";
            }
    }
    return 0;
}

